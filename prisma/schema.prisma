generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  email     String    @unique
  password  String
  createdAt DateTime  @default(now())
  name      String?
  id        Int       @id @default(autoincrement())
  accounts  Account[]
}

model Account {
  provider          String
  providerAccountId String
  id                Int    @id @default(autoincrement())
  userId            Int
  user              User   @relation(fields: [userId], references: [id], onDelete: Cascade)
}
